        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="127dp"
        tools:layout_editor_absoluteY="243dp" />

    <ImageView
        android:id="@+id/fto1"
        android:layout_width="126dp"
        android:layout_height="150dp"
        android:background="#FFFFFF"

        android:scaleType="fitXY"
        android:src="@drawable/mmc"
        app:srcCompat="@drawable/mmc"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="1dp"
        tools:layout_editor_absoluteY="204dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="4,4 "
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="127dp"
        tools:layout_editor_absoluteY="223dp" />

    <androidx.appcompat.widget.AppCompatImageView
        android:layout_width="20dp"
        android:layout_height="20dp"
        android:src="@drawable/stars"
        app:srcCompat="@drawable/stars"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="149dp"
        tools:layout_editor_absoluteY="223dp" />

    <Button
        android:id="@+id/buttonMMC"
        android:layout_width="284dp"
        android:layout_height="47dp"
        android:backgroundTint="@color/light_blue_500"
        android:text="See More"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="128dp"
        tools:layout_editor_absoluteY="300dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="RS Metropolitan Medical Centre (MMC)"
        android:textStyle="bold"

        tools:layout_editor_absoluteX="127dp"
        tools:layout_editor_absoluteY="204dp"
        tools:ignore="MissingConstraints" />


    <TextView
        android:id="@+id/textView_RumahSakit"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Layanan Rumah Sakit"
        android:textAllCaps="true"
        android:textColor="#FFFFFF"
        android:textSize="24sp"
        android:textStyle="bold"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="68dp"
        tools:layout_editor_absoluteY="67dp" />

</androidx.constraintlayout.widget.ConstraintLayout>